    <class>ec.edu.uce.medicina.seguimiento.modelo.TipoPregunta</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Universidad</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Usuario</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
      <property name="hibernate.show_sql" value="true"/>
      <property name="hibernate.format_sql" value="true"/>
    </properties>
  </persistence-unit>
</persistence>
