    <class>ec.edu.uce.medicina.seguimiento.modelo.Ciudad</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Encuesta</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.EstadoCivil</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Facultad</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Paginas</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Pais</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Perfil</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.PerfilPaginas</class>
    <class>ec.edu.uce.medicina.seguimiento.modelo.Persona</class>
